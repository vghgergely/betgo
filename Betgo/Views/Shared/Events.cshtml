@model IEnumerable<Betgo.Models.Event>

<ul>
@foreach (var events in Model)
{
    <li>
        <div class="eventName">@events.Name</div>
        <div class="competitor">@events.CompetitorA</div>
        <div class="competitor">@events.CompetitorB</div>
        <div class="date">@events.Date</div>
        <div class="time">@events.Time</div>
        <div class="type">@events.Type.Name</div>
        <hr/>
    </li>
    }
</ul>